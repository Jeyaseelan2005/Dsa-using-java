class Solution {
    public int findMaxConsecutiveOnes(int[] nums) {

        //Optimal solution - tc : O(n)

        int size = nums.length;
        int count = 0;
        int maxs = 0;

        for(int i=0;i<size;i++) {
            if(nums[i]==1) {
                count++;
            }
            else {
                count = 0;
            }
            maxs = Math.max(count,maxs);
        }
        return maxs;
    }
}
