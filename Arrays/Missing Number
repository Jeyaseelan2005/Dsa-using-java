//By using Formula - n*(n+1) / 2  tc : O(n)

class Solution {
    public int missingNumber(int[] nums) {
        int size  = nums.length;
        int sum = 0;

        for(int i=0;i<size;i++) {
            sum+=nums[i];
        }

        int result = size*(size+1)/2 - sum;

        return result;
    }
}
