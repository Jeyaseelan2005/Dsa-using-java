//optimal solution - tc : O(n)

public class Solution {

    static int[] rotateArray(int[] arr, int n) {
        // Write your code here.
        int temp = arr[0];
        int i;
        
        for(i=1;i<n;i++) {
            arr[i-1] = arr[i];
        }
        arr[n-1] = temp;

        return arr;
    }
}
